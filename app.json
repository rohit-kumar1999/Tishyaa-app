{
  "expo": {
    "name": "Tishyaa Jewels",
    "slug": "tishyaa-jewels",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#C9A961"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tishyaajewels.app",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to take product photos",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select product images",
        "NSLocationWhenInUseUsageDescription": "This app needs location access for store locator and delivery tracking",
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": [
          "tez",
          "phonepe",
          "paytmmp",
          "gpay",
          "bhim",
          "upi"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "package": "com.tishyaajewels.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "USE_FINGERPRINT",
        "USE_BIOMETRIC",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VISUAL_USER_SELECTED",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-camera",
      "expo-media-library",
      "expo-notifications",
      "expo-location",
      "expo-local-authentication",
      "expo-secure-store",
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "df5fa61e-30a9-4745-80a8-8d1ad6fbb819"
      }
    },
    "owner": "rohitaryal1999"
  }
}
